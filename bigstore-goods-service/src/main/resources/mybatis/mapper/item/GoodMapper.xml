<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="GoodMapper">
	<!--增加商品品牌-->
	<insert id="add" parameterType="com.sqt.pojo.TbGoods" useGeneratedKeys="true" keyProperty="id">
		INSERT INTO `tb_goods` (
			  `seller_id`,
			  `goods_name`,
			  `default_item_id`,
			  `audit_status`,
			  `is_marketable`,
			  `brand_id`,
			  `caption`,
			  `category1_id`,
			  `category2_id`,
			  `category3_id`,
			  `small_pic`,
			  `price`,
			  `type_template_id`,
			  `is_enable_spec`,
			  `is_delete`
			)
			VALUES
			  (
			    #{sellerId},
			    #{goodsName},
			    #{defaultItemId},
			    #{auditStatus},
			    #{isMarketable},
			    #{brandId},
			    #{caption},
			    #{category1Id},
			    #{category2Id},
			    #{category3Id},
			    #{smallPic},
			    #{price},
			    #{typeTemplateId},
			    #{isEnableSpec},
			    #{isDelete}
			  );

	</insert>
</mapper>